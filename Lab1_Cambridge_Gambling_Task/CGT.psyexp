        <Param val="# --- origion meney will be 100 --- &amp;#10;token = 100&amp;#10;&amp;#10;# --- config position of bule and red cards ---&amp;#10;posl = [[-0.45, 0.25],&amp;#10;        [-0.35, 0.25],&amp;#10;        [-0.25, 0.25],&amp;#10;        [-0.15, 0.25],&amp;#10;        [-0.05, 0.25],&amp;#10;        [0.05, 0.25],&amp;#10;        [0.15, 0.25],&amp;#10;        [0.25, 0.25],&amp;#10;        [0.35, 0.25],&amp;#10;        [0.45, 0.25]]&amp;#10;&amp;#10;# --- stimuli deifination --- &amp;#10;# not only save words but also easy to modify&amp;#10;stims = ['stimuli/red.png', 'stimuli/blue.png']&amp;#10;cstims = ['stimuli/rc.jpg', 'stimuli/bc.jpg']&amp;#10;ncstims = ['stimuli/r.jpg', 'stimuli/b.jpg']&amp;#10;&amp;#10;&amp;#10;# --- count down time --- &amp;#10;ct1 = 5 # aka, time interval&amp;#10;ct2 = ct1 * 2&amp;#10;ct3 = ct1 * 3&amp;#10;ct4 = ct1 * 4&amp;#10;ct5 = ct1 * 5&amp;#10;&amp;#10;&amp;#10;# --- rate sequence ---&amp;#10;counthighlow = [.95 , .75, .5, .25, .05]&amp;#10;countlowhigh = [0.05, 0.25, .5, 0.75, 0.95]&amp;#10;&amp;#10;# --- stake number (position) ---&amp;#10;bpos = [.24, 0.08]&amp;#10;&amp;#10;# --- arrow position ---&amp;#10;arrPos = [0.5, -0.35]&amp;#10;&amp;#10;# --- other info --- &amp;#10;# red card =&gt; left position (-0.2, 0)&amp;#10;# blue card =&gt; right position (0.2, 0)&amp;#10;&amp;#10;" valType="extendedCode" updates="constant" name="Begin Routine"/>
